{
  "type": "sqlite",
  "database": "./data/game_resources.sqlite",
  "synchronize": true,
  "logging": true,
  "entities": [
    "src/models/*.ts"
  ],
  "migrations": [
    "src/migrations/*.ts"
  ],
  "subscribers": [
    "src/subscribers/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/models",
    "migrationsDir": "src/migrations",
    "subscribersDir": "src/subscribers"
  }
}